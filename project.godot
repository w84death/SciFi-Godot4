; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Sci-Fi Scene"
run/main_scene="res://main.tscn"
config/icon="res://icon.png"

[display]

window/size/width=1280
window/size/height=800
window/stretch/mode="viewport"

[rendering]

global_illumination/gi/use_half_resolution=true
global_illumination/voxel_gi/quality=0
camera/depth_of_field/depth_of_field_bokeh_shape=2
camera/depth_of_field/depth_of_field_bokeh_quality=2
occlusion_culling/bvh_build_quality=1
anti_aliasing/quality/msaa=2
anti_aliasing/quality/screen_space_aa=1
occlusion_culling/use_occlusion_culling=true
environment/defaults/default_environment="res://default_env.tres"
